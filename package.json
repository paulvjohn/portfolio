{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "env": "env",
    "clean": "rm -fr ./dist/*",
    "build:scripts": "./node_modules/webpack/bin/webpack.js --progress --colors",
    "build:styles": "./node_modules/less/bin/lessc --clean-css ./src/frontend/index.less ./dist/bundle.css",
    "build:images": "cp ./src/frontend/images/*.* ./dist/",
    "build:fonts": "cp ./src/frontend/fonts/*.* ./dist/",
    "build:html": "cp ./src/frontend/*.html ./dist/",
    "build": "npm run clean && ./node_modules/parallelshell/index.js 'npm run build:scripts' 'npm run build:styles' 'npm run build:fonts' 'npm run build:images' 'npm run build:html'",
    "fb-flo": "node flo.js",
    "test": "./node_modules/mocha/bin/mocha -u bdd -R list",
    "watch:server": "./node_modules/nodemon/bin/nodemon.js -w src/backend/ --ext 'js' --exec 'npm run start:debug'",
    "watch:scripts": "./node_modules/webpack/bin/webpack.js --config webpack.dev.config.js --progress --colors --watch",
    "watch:test": "./node_modules/mocha/bin/mocha -w -u bdd -R list",
    "watch:styles": "./node_modules/watchy/bin/watchy -w ./src/frontend/styles/,./src/frontend/index.less -- ./node_modules/less/bin/lessc --source-map ./src/frontend/index.less ./dist/bundle.css",
    "watch:html": "./node_modules/watchy/bin/watchy -w ./src/frontend/index.html -- npm run build:html",
    "watch:images": "./node_modules/watchy/bin/watchy -w ./src/frontend/images/ -- npm run build:images",
    "watch:fonts": "./node_modules/watchy/bin/watchy -w ./src/frontend/fonts/ -- npm run build:fonts",
    "watch": "./node_modules/parallelshell/index.js 'npm run watch:server' 'npm run watch:scripts' 'npm run watch:test' 'npm run watch:styles' 'npm run watch:html' 'npm run watch:images' 'npm run watch:fonts' 'npm run fb-flo'",
    "start": "node --harmony ./src/backend/server.js",
    "start:debug": "node --harmony --debug ./src/backend/server.js"
  },
  "devDependencies": {
    "6to5": "^3.6.4",
    "6to5-loader": "^3.0.0",
    "del": "^1.1.1",
    "dotenv": "^0.5.1",
    "fb-flo": "^0.4.0",
    "jsx-loader": "^0.12.2",
    "less-plugin-clean-css": "^1.5.0",
    "mocha": "^2.1.0",
    "nodemon": "^1.3.7",
    "parallelshell": "^1.0.4",
    "require-dir": "^0.1.0",
    "should": "^5.0.0",
    "watchy": "^0.6.0",
    "webpack": "^1.5.3"
  },
  "dependencies": {
    "bignumber.js": "^2.0.3",
    "body-parser": "^1.12.0",
    "capitalize": "^0.5.0",
    "compression": "^1.4.1",
    "connect-redis": "^2.2.0",
    "console-browserify": "^1.1.0",
    "cookie": "^0.1.2",
    "cookie-parser": "^1.3.4",
    "del": "^1.1.1",
    "dotenv": "^0.5.1",
    "express": "^4.12.2",
    "express-session": "^1.10.3",
    "faker": "^2.1.2",
    "forever": "^0.14.1",
    "forever-monitor": "^1.5.2",
    "griddle-react": "^0.2.4",
    "immutable": "^3.6.2",
    "jade": "^1.9.2",
    "keymaster": "^1.6.2",
    "less": "^2.4.0",
    "lodash": "^3.3.1",
    "moment": "^2.9.0",
    "morgan": "^1.5.1",
    "multer": "^0.1.8",
    "mysql": "^2.5.5",
    "node-easypost": "^2.0.5",
    "numeral": "^1.5.3",
    "pdfkit": "^0.7.0",
    "pm2": "^0.12.7",
    "pmx": "^0.3.4",
    "raven": "^0.7.2",
    "react": "^0.12.2",
    "react-bootstrap": "^0.14.1",
    "react-bootstrap-datetimepicker": "0.0.9",
    "react-datepicker": "^0.5.1",
    "react-fontawesome": "0.0.3",
    "react-router": "^0.12.4",
    "react-tools": "^0.13.0-rc2",
    "redis": "^0.12.1",
    "reflux": "^0.2.5",
    "request": "^2.53.0",
    "request-json": "^0.5.2",
    "rickshaw": "^1.5.1",
    "sequelize": "^2.0.3",
    "sequelize-cli": "^1.3.1",
    "socket.io": "^1.3.5",
    "uuid": "^2.0.1",
    "validator": "^3.33.0",
    "winston": "^0.9.0"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=2.0.0"
  },
  "private": true
}
